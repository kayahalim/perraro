<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="https://cdn.babylonjs.com/babylon.js"></script>
  <script src="https://cdn.babylonjs.com/loaders/babylonjs.loaders.min.js"></script>
</head>
<body>
  <div id="3dModelContainer" style="width: 100%; height: 500px;"></div>

  <script>
    var canvas = document.getElementById("3dModelContainer");
    var engine = new BABYLON.Engine(canvas, true);

    BABYLON.SceneLoader.Append("", "https://github.com/kayahalim/perraro/raw/main/ImageToStl.com_elktrikli+motor+3.fbx", engine, function(scene) {
      engine.runRenderLoop(function() {
        scene.render();
      });
    });
  </script>
</body>
</html>
